<Button>:
    font_size: 40
    size_hint: 0.1, 0.45

<RoundButton>:
    size_hint: 0.1 * 1.27, 0.045 * 1.27
    font_color: 0, 0, 0, 0
    background_color: 0, 0, 0, 0
    on_press: root.buttonPressed()
    on_release: root.buttonReleased()
    canvas.before:
        PushMatrix
        Rotate:
            angle: 90
            origin: self.center
    canvas.after:
        PopMatrix

    Image:
        id: round_button_texture
        allow_stretch: True
        source: 'textures/buttons/roundbutton_redorange_up.png'
        center_x: self.parent.center_x
        center_y: self.parent.center_y
        size: self.parent.size

